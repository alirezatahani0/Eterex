{
	"name": "eterex",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3001",
		"build": "next build",
		"start": "next start",
		"lint": "eslint",
		"scrape:coins": "node scripts/scrape-coins.js",
		"scrape:coins:puppeteer": "node scripts/scrape-coins-puppeteer.js",
		"convert:html-to-json": "node scripts/html-to-json.js",
		"docker:build": "docker build -t eterex:latest .",
		"docker:build:args": "docker build --build-arg NEXT_PUBLIC_SITE_URL=${NEXT_PUBLIC_SITE_URL} --build-arg NEXT_PUBLIC_API_URL=${NEXT_PUBLIC_API_URL} --build-arg NEXT_PUBLIC_PUBLIC_API_URL=${NEXT_PUBLIC_PUBLIC_API_URL} --build-arg NEXT_PUBLIC_STAKING_API_URL=${NEXT_PUBLIC_STAKING_API_URL} --build-arg NEXT_PUBLIC_BASE_PATH=${NEXT_PUBLIC_BASE_PATH} --build-arg NEXT_PUBLIC_ASSET_PREFIX=${NEXT_PUBLIC_ASSET_PREFIX} --build-arg NEXT_PUBLIC_ICON_BASE_URL=${NEXT_PUBLIC_ICON_BASE_URL} --build-arg NEXT_PUBLIC_GAPIFY_WEBSITE_TOKEN=${NEXT_PUBLIC_GAPIFY_WEBSITE_TOKEN} --build-arg GOOGLE_VERIFICATION=${GOOGLE_VERIFICATION} --build-arg NEXT_PUBLIC_GTM_ID=${NEXT_PUBLIC_GTM_ID} --build-arg NEXT_PUBLIC_GA_MEASUREMENT_ID=${NEXT_PUBLIC_GA_MEASUREMENT_ID} -t eterex:latest .",
		"docker:run": "docker run -p 3000:3000 eterex:latest",
		"docker:compose": "docker-compose up -d",
		"docker:compose:build": "docker-compose up -d --build"
	},
	"dependencies": {
		"@next/third-parties": "^16.1.6",
		"@tanstack/react-query": "^5.90.16",
		"@types/leaflet": "^1.9.21",
		"@vercel/speed-insights": "1.3.1",
		"framer-motion": "^12.34.0",
		"leaflet": "^1.9.4",
		"next": "^16.1.1",
		"primereact": "^10.9.7",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-leaflet": "^5.0.0",
		"react-ticker": "^1.3.2",
		"swiper": "^12.0.3"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"cheerio": "^1.2.0",
		"eslint": "^9",
		"eslint-config-next": "16.1.1",
		"puppeteer": "^24.34.0",
		"tailwindcss": "^4",
		"typescript": "^5"
	}
}
